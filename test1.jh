entity User1 {
	id String
    nume String
    prenume String
    cnp String
}

entity UserType {
	id String
    type String
}

entity Company {
	id String
    name String
}

entity Client {
	id String
    name String
}

relationship ManyToOne {
	User1 to UserType{user}
    User1 to Company{user}
    Client to Company{client}
}


// Set pagination options
paginate User with infinite-scroll
paginate Client with pagination

